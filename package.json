{
  "name": "swanhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run serve\n",
    "serve_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run serve\n",
    "build:prod": "vue-cli-service build --mode production",
    "build:prod_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:prod\n",
    "build:prod_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:prod\n",
    "build:testnet": "vue-cli-service build --mode testnet",
    "build:testnet_t": "set NODE_OPTIONS=\"--openssl-legacy-provider\" & npm run build:testnet\n",
    "build:testnet_u": "export NODE_OPTIONS=\"--openssl-legacy-provider\" && npm run build:testnet\n",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@wagmi/connectors": "^4.3.10",
    "@wagmi/core": "^2.9.8",
    "@wagmi/vue": "^0.0.3",
    "@web3modal/wagmi": "^4.2.1",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.6.5",
    "echarts": "^5.4.2",
    "element-plus": "^2.6.1",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "moment": "^2.29.4",
    "viem": "^2.10.9",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.9",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "web3modal": "^1.9.12"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/eslint-parser": "7.12.16",
    "@intlify/vue-i18n-loader": "^3.0.0", 
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.27",
    "compression-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "3.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-cli-plugin-i18n": "~2.3.1", 
    "eslint": "7.32.0",
    "eslint-plugin-vue": "8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
